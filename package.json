{
  "name": "obsidian-journal",
  "version": "2.1.7",
  "description": "Journal plugin for Obsidian (https://obsidian.md)",
  "main": "main.js",
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "version": "node version-bump.mjs && git add manifest.json manifest-beta.json versions.json",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "bench": "vitest bench",
    "ts-check": "vue-tsc -b",
    "changelog": "npx git-cliff --output CHANGELOG.md",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:srg-kostyrko/obsidian-journal.git"
  },
  "keywords": [
    "Obsidian",
    "plugin",
    "journal"
  ],
  "author": "Sergii Kostyrko",
  "license": "MIT",
  "dependencies": {
    "@vee-validate/valibot": "^4.15.0",
    "@vueuse/core": "^13.1.0",
    "eslint-plugin-vue": "^10.1.0",
    "obsidian": "^1.8.7",
    "perfect-debounce": "^1.0.0",
    "valibot": "^1.0.0",
    "vee-validate": "^4.15.0",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@cspell/eslint-plugin": "^8.19.3",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.23.0",
    "@tsconfig/node20": "^20.1.5",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitest/coverage-v8": "^3.1.2",
    "@vue/tsconfig": "^0.7.0",
    "builtin-modules": "^5.0.0",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-unicorn": "^59.0.0",
    "git-cliff": "^2.8.0",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.1",
    "prettier": "^3.5.3",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.1",
    "vite": "^6.3.4",
    "vite-plugin-static-copy": "^2.3.1",
    "vitest": "^3.1.2",
    "vue-tsc": "^2.2.10"
  },
  "lint-staged": {
    "*.{ts,vue}": "eslint",
    "*.{ts,mjs,js,css,md,vue}": "prettier --write"
  }
}
