{
  "name": "obsidian-journal",
  "version": "1.4.0",
  "description": "Journal plugin for Obsidian (https://obsidian.md)",
  "main": "main.js",
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest",
    "bench": "vitest bench",
    "ts-check": "vue-tsc -b",
    "changelog": "npx git-cliff --output CHANGELOG.md",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:srg-kostyrko/obsidian-journal.git"
  },
  "keywords": [
    "Obsidian",
    "plugin",
    "journal"
  ],
  "author": "Sergii Kostyrko",
  "license": "MIT",
  "dependencies": {
    "@vee-validate/valibot": "4.13.2",
    "@vueuse/core": "^11.3.0",
    "obsidian": "^1.7.2",
    "perfect-debounce": "^1.0.0",
    "valibot": "0.42.1",
    "vee-validate": "4.13.2",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.12.0",
    "@tsconfig/node20": "^20.1.4",
    "@typescript-eslint/eslint-plugin": "^8.0.0-alpha.40",
    "@typescript-eslint/parser": "^8.0.0-alpha.39",
    "@vitejs/plugin-vue": "^5.2.0",
    "@vue/tsconfig": "^0.6.0",
    "builtin-modules": "^4.0.0",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-unicorn": "^56.0.1",
    "eslint-plugin-vue": "^9.31.0",
    "git-cliff": "^2.6.1",
    "globals": "^15.12.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "tslib": "^2.8.1",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.15.0",
    "vite": "^5.4.11",
    "vite-plugin-static-copy": "^2.1.0",
    "vitest": "^2.1.5",
    "vue-tsc": "^2.1.10"
  },
  "lint-staged": {
    "*.{ts,vue}": "eslint",
    "*.{ts,mjs,js,css,md,vue}": "prettier --write"
  }
}
